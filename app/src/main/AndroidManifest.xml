<?xml version="1.0" encoding="utf-8"?>
<manifest package="edu.nsu.ir"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="edu.nsu.ir.MainActivity">
        </activity>
        <activity android:name=".LoginSignin.LSLoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginSignin.LSSignupActivity"
            android:parentActivityName=".LoginSignin.LSLoginActivity">
        </activity>
        <activity
            android:name=".Cases.CCaseHomeActivity"
            android:parentActivityName=".LoginSignin.LSLoginActivity">
        </activity>
        <activity
            android:name=".Cases.CNewCaseActivity"
            android:parentActivityName=".Cases.CCaseHomeActivity">
        </activity>
        <activity
            android:name=".Cases.CQuestionActivity"
            android:parentActivityName=".Cases.CNewCaseActivity">
        </activity>
        <activity
            android:name=".Cases.CCaseAnalysisActivity"
            android:parentActivityName=".Cases.CCaseAnalysisActivity">
        </activity>
        <activity
            android:name=".Cases.CShareCaseActivity"
            android:parentActivityName=".Cases.CCaseHomeActivity">
        </activity>
        <activity
            android:name=".Cases.CCaseDetailsActivity"
            android:parentActivityName=".Cases.CCaseHomeActivity">
        </activity>
    </application>

</manifest>
